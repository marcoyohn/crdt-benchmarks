{
  "name": "yjs",
  "version": "1.0.0",
  "description": "Yjs benchmarks",
  "main": "./run.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "([ -f \"./dist/bundle.js\" ] || npm run dist) && node ../../bin/measure-bundle.js && node --expose-gc run.js && npm run table",
    "table": "echo 'Yjs results: \n\n' && node ../../bin/render-table.js ../results.json 6000 yjs",
    "dist": "npm run clean && rollup -c && gzip --keep dist/*"
  },
  "author": "Kevin Jahns",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.5.0",
    "yjs": "^13.5.12",
    "y-websocket": "^1.5.0",
    "y-protocols": "^1.0.5",
    "@hocuspocus/provider": "^2.4.0"
  }
}
